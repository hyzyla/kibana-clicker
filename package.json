{
  "name": "kibana-clicker",
  "displayName": "Kibana Clicker",
  "version": "100.0.7",
  "description": "Automation tool for Kibana",
  "author": "Yevhenii Hyzyla <hyzyla@gmail.com>",
  "homepage": "https://github.com/hyzyla/kibana-clicker",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:chrome": "wxt zip",
    "build:firefox": "wxt zip -b firefox",
    "lint": "eslint entrypoints/ utils/",
    "lint:fix": "eslint entrypoints/ utils/ --fix",
    "format": "prettier --write \"entrypoints/**/*.{ts,tsx,css}\" \"utils/**/*.ts\" \"*.{json,mjs,ts}\"",
    "format:check": "prettier --check \"entrypoints/**/*.{ts,tsx,css}\" \"utils/**/*.ts\" \"*.{json,mjs,ts}\"",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "rison": "^0.1.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.7.1",
    "@types/rison": "0.1.0",
    "eslint": "9.27.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.1",
    "wxt": "^0.20.18"
  }
}
