{
  "name": "kibana-clicker",
  "displayName": "Kibana Clicker",
  "version": "100.0.7",
  "description": "Automation tool for Kibana",
  "author": "Yevhenii Hyzyla <hyzyla@gmail.com>",
  "homepage": "https://github.com/hyzyla/kibana-clicker",
  "scripts": {
    "dev": "plasmo dev",
    "build:chrome": "plasmo build --target=chrome-mv3 --zip",
    "build:firefox": "plasmo build --target=firefox-mv2 --zip",
    "test": "plasmo test",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\" \"*.{json,mjs}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\" \"*.{json,mjs}\""
  },
  "dependencies": {
    "plasmo": "^0.90.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rison": "^0.1.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.7.1",
    "@types/chrome": "0.0.306",
    "@types/node": "22.15.3",
    "@types/react": "18.3.28",
    "@types/react-dom": "18.3.7",
    "@types/rison": "0.1.0",
    "eslint": "9.27.0",
    "prettier": "3.5.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.1"
  },
  "manifest": {
    "applications": {
      "gecko": {
        "id": "kibana-clicker@hyzyla.dev"
      }
    },
    "browser_specific_settings": {
      "gecko": {
        "id": "kibana-clicker@hyzyla.dev"
      }
    }
  }
}
